    "react-transition-group": "^2.5.0",
    "styled-components": "^3.4.9",
    "styled-media-query": "^2.1.1",
    "styled-tools": "^1.6.0",
    "sweetalert": "^2.1.2",
    "waait": "^1.0.2"
